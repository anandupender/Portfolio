{
  "name": "AnandUpenderPersonalSite",
  "version": "1.0.0",
  "description": "My Personal Evolving Portfolio created in Dec. 2018 and refined constantly",
  "main": "gulpfile.js",
  "paths": {
    "generic": {
      "src": "./src",
      "dest": "./dist"
    },
    "markup": {
      "src": "./src/*.html",
      "dest": "./dist"
    },
    "files": {
      "src": "./src/data/*.*",
      "dest": "./dist/data"
    },
    "styles": {
      "src": "./src/sass/*.scss",
      "dest": "./dist/css"
    },
    "scripts": {
      "src": "./src/js/*.js",
      "dest": "./dist/js"
    },
    "copyImages": {
      "src": "./src/optimizedImages/**/*.*",
      "dest": "./dist/images"
    },
    "unsupportedImages": {
      "src": [
        "./src/images/**/*.{gif,svg,ico,txt,pdf}"
      ],
      "dest": "./src/optimizedImages"
    },
    "images": {
      "src": "./src/images/**/*.{jpg,webp,JPG,jpeg}",
      "dest": "./src/optimizedImages"
    },
    "oneTimeImages": {
      "src": "./src/images/stream/*.{jpg,JPG,jpeg,png}",
      "dest": "./src/optimizedImages/tempBatch"
    },
    "pngs": {
      "src": "./src/images/**/*.png",
      "dest": "./src/optimizedImages"
    },
    "largeImages": {
      "src": "./src/images/**/hero.{png,jpg,jpeg}",
      "dest": "./src/optimizedImages"
    }
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anandupender/Portfolio.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/anandupender/Portfolio/issues"
  },
  "homepage": "https://github.com/anandupender/Portfolio#readme",
  "devDependencies": {
    "browser-sync": "^2.26.14",
    "concurrent-transform": "^1.0.0",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-htmlmin": "^5.0.1",
    "gulp-image-resize": "^0.13.1",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.1.0",
    "gulp-uglify": "^3.0.2",
    "gulp-webp": "^4.0.1",
    "node-sass": "^5.0.0"
  },
  "dependencies": {
    "@swup/scripts-plugin": "^1.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^6.2.0",
    "gulp-uglify-es": "^2.0.0",
    "os": "^0.1.1"
  }
}
